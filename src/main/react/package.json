        "coveralls": "3.0.2",
        "css-loader": "1.0.0",
        "enzyme": "3.7.0",
        "enzyme-adapter-react-16": "1.6.0",
        "enzyme-to-json": "3.3.4",
        "eslint": "5.7.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-config-prettier": "3.1.0",
        "eslint-import-resolver-webpack": "0.10.1",
